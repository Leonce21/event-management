generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  location    String?
  startDate   DateTime
  endDate     DateTime
  isOnline    Boolean  @default(false)
  capacity    Int?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
